<Effect name="Relief" type="Vintage" id="91">
    <Allocation name="technicolor"/>
    
    <BaseFilter name="Technicolor">
        <src>SRC</src>
        <dst>technicolor</dst>
        <params>
            <technicolorRed>0.992, 0.137, 0.314</technicolorRed>
            <technicolorGreen>0.204, 0.98, 0.725</technicolorGreen>
        </params>
    </BaseFilter>
    
    <BaseFilter name="Vignette">
        <src>technicolor</src>
        <dst>DST</dst>
        <params>
            <mode>linear</mode>
            <vignetteStart>0.0</vignetteStart>
            <vignetteRange>0.7</vignetteRange>
        </params>
    </BaseFilter>
</Effect>