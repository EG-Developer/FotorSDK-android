<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/fotor_feature_effect_operation_panel" />

    <FrameLayout
        android:id="@+id/fotor_fx_effect_superposed_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/fotor_fx_effect_superposed_choose_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:visibility="gone" >

        <com.everimaging.fotorsdk.widget.lib.hlist.HListView
            android:id="@+id/fotor_fx_effect_superposed_choose_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:cacheColorHint="@color/fotor_transparent"
            android:listSelector="@drawable/fotor_transparent" />
    </FrameLayout>

</FrameLayout>